  <legend>Participant Info</legend> 
  <% form_tag do  %>
    <div>
      <%= f.label :name %>
      <%= f.text_field :name, :readonly => "readonly" %>
    </div>
  
    <div>
      <%= f.label :address %>
      <%= f.text_field :address, :readonly => "readonly" %>
    </div>
  
    <div>
      <%= f.label :province %>
      <%= 
        @provinces = Province.all
        f.collection_select(:province_id, @provinces, :id, :name)
      %>
    </div>
  
    <div>
      <%= f.label :postal_code %>
      <%= f.text_field :postal_code, :readonly => "readonly" %>
    </div>
  
    <div>
      <%= f.label :phone_number %>
      <%= f.text_field :phone_number, :readonly => "readonly" %>
    </div>
  
    <div>
      <%= f.label :email %>
      <%= f.text_field :email, :readonly => "readonly" %>
    </div>
  <% end %>
</fieldset>
<br/>
<fieldset> 
  <legend>Species Info</legend>
  <% form_for (@bap_species) do |f| %>
    <div> 
      <%= f.label :latin_name, "Reported Species (Latin Name)" %>
      <%= f.text_field :latin_name %>
    </div>
  <% end %>
  
  
</fieldset> 
