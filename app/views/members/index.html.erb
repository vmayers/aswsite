<legend>Listing members</legend>
<p><%= link_to 'New Member', new_member_path %></p>
<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>Province</th>
    <th>Phone number</th>
    <th>Postal code</th>
    <th>Email</th>
    <th></th>
  </tr>

<% @members.each do |member| %>
  <tr>
    <td><%= member.name %></td>
    <td><%= member.address %></td>
    <td><%= member.province_id %></td>
    <td><%= member.phone_number %></td>
    <td><%= member.postal_code %></td>
    <td><%= member.email %></td>
    <td class="item-actions">
      <%= link_to 'Show', member %> |
      <%= link_to 'Edit', edit_member_path(member) %> |
      <%= link_to 'Destroy', member, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

